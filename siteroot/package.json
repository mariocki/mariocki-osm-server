{
    "_moduleAliases": {
        "@server": "dist/Server",
        "@shared": "dist/shared"
    },
    "dependencies": {
        "@azure/msal-node": "^1.1.0",
        "command-line-args": "^5.1.1",
        "cookie-parser": "^1.4.5",
        "dotenv": "^10.0.0",
        "ejs": "^3.1.6",
        "esri-leaflet": "^3.0.2",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-session": "^1.17.2",
        "helmet": "^4.6.0",
        "http-status-codes": "^2.1.4",
        "jet-logger": "^1.0.6",
        "jsonfile": "^6.1.0",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^1.11.0",
        "leaflet": "^1.7.1",
        "leaflet-geosearch": "^3.3.2",
        "localenvify": "^1.0.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/command-line-args": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/esri-leaflet": "^2.1.8",
        "@types/express": "^4.17.12",
        "@types/express-session": "^1.17.0",
        "@types/find": "^0.2.1",
        "@types/fs-extra": "^9.0.11",
        "@types/jasmine": "^3.7.7",
        "@types/jsonfile": "^6.0.0",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/leaflet": "^1.7.2",
        "@types/leaflet-geosearch": "^3.0.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^15.12.2",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "eslint": "^7.28.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.4",
        "find": "^0.3.0",
        "fs-extra": "^10.0.0",
        "jasmine": "^3.7.0",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.3.2"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:@typescript-eslint/recommended-requiring-type-checking"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/explicit-module-boundary-types": 0,
            "@typescript-eslint/no-explicit-any": 0,
            "@typescript-eslint/no-floating-promises": 0,
            "@typescript-eslint/no-unsafe-assignment": 0,
            "@typescript-eslint/no-unsafe-member-access": 0,
            "@typescript-eslint/restrict-plus-operands": 0,
            "max-len": [
                "error",
                {
                    "code": 100
                }
            ],
            "no-console": 1,
            "no-extra-boolean-cast": 0
        }
    },
    "eslintIgnore": [
        "src/public/",
        "build.ts"
    ],
    "name": "siteroot2",
    "nodemonConfig": {
        "exec": "./node_modules/.bin/ts-node -r module-alias/register ./src",
        "ext": "js, ts, html",
        "ignore": [
            "src/public"
        ],
        "watch": [
            "src"
        ]
    },
    "scripts": {
        "build": "cp -aR node_modules/leaflet/dist/images src/public/stylesheets/ && cp node_modules/leaflet-geosearch/dist/geosearch.css src/public/stylesheets/ && cp node_modules/leaflet/dist/leaflet.css src/public/stylesheets/ && ./node_modules/.bin/ts-node build.ts && browserify dist/public/scripts/webmap.js -t [localenvify --envfile dist/pre-start/env/.env ] -o dist/public/scripts/webmap-browser.js",
        "lint": "eslint . --ext .ts",
        "start": "node -r module-alias/register ./dist",
        "start:dev": "nodemon"
    },
    "version": "0.0.0"
}
